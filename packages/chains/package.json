{
  "name": "@coinbox/chains",
  "version": "1.0.0",
  "description": "Coinbox chain registry - single source of truth for blockchain network data",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  },
  "keywords": [
    "blockchain",
    "chains",
    "ethereum",
    "bitcoin",
    "registry"
  ],
  "license": "MIT"
}
